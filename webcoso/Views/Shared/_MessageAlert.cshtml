@using webcoso.Message

@if (Notification.has_flash())
{
    ModelNotification Notifi = Notification.get_flash();
    string message = Notifi.mgs.ToString();
    string type = Notifi.mgs_type.ToString();
    <script>
        Swal.fire(
            'Thông báo!',
            '@message',
            '@type',
    )
    </script>
}